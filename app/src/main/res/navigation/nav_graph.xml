<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.instagram.fragments.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_commentSheet"
            app:destination="@id/commentSheet" />
        <action
            android:id="@+id/action_homeFragment_to_profileFragment"
            app:destination="@id/profileFragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right" />
    </fragment>
    <fragment
        android:id="@+id/postFragment"
        android:name="com.example.instagram.fragments.PostFragment"
        android:label="fragment_post"
        tools:layout="@layout/fragment_post" >
        <action
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right"
            android:id="@+id/action_postFragment_to_captionTagFragment"
            app:destination="@id/captionTagFragment" />
    </fragment>
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.example.instagram.fragments.SearchFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search">
        <action
            android:id="@+id/action_searchFragment_to_profileFragment"
            app:destination="@id/profileFragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right"
            app:popUpTo="@id/searchFragment"
            app:popUpToInclusive="false" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.instagram.fragments.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile">
        <action
            android:id="@+id/action_profileFragment_to_onePostFragment"
            app:destination="@id/onePostFragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right" />
        <argument
            android:name="profileId"
            app:argType="long" />
        <action
            android:id="@+id/action_profileFragment_to_profileMenu2"
            app:destination="@id/profileMenu2" />
        <action
            android:id="@+id/action_profileFragment_to_editProfileFragment"
            app:destination="@id/editProfileFragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right" />
        <action
            android:id="@+id/action_profileFragment_to_listFollowFragment"
            app:destination="@id/listFollowFragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right" />
        <action
            android:id="@+id/action_profileFragment_to_profilePictureFragment"
            app:destination="@id/profilePictureFragment" />
    </fragment>
    <fragment
        android:id="@+id/onePostFragment"
        android:name="com.example.instagram.fragments.OnePostFragment"
        android:label="OnePostFragment"
        tools:layout="@layout/fragment_one_post">
        <argument
            android:name="postId"
            app:argType="long" />
        <action
            android:id="@+id/action_onePostFragment_to_profileFragment"
            app:destination="@id/profileFragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right"
            app:popUpTo="@id/profileFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_onePostFragment_to_commentSheet"
            app:destination="@id/commentSheet" />
        <argument
            android:name="pos"
            app:argType="integer" />
    </fragment>
    <dialog
        android:id="@+id/commentSheet"
        android:name="com.example.instagram.bottomsheet.CommentSheet"
        android:label="CommentSheet"
        tools:layout="@layout/bottomsheet_comments">
        <argument
            android:name="postId"
            app:argType="long" />
        <action
            android:id="@+id/action_commentSheet_to_profileFragment"
            app:destination="@id/profileFragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right"
            app:popUpTo="@id/profileFragment"
            app:popUpToInclusive="true" />
    </dialog>
    <dialog
        android:id="@+id/profileMenu2"
        android:name="com.example.instagram.bottomsheet.ProfileMenu"
        android:label="ProfileMenu"
        tools:layout="@layout/bottomsheet_my_profile_menu">
        <action
            android:id="@+id/action_profileMenu2_to_savedPostFragment"
            app:destination="@id/savedPostFragment" />
        <action
            android:id="@+id/action_profileMenu2_to_settingsFragment"
            app:destination="@id/settingsFragment" />
    </dialog>
    <fragment
        android:id="@+id/savedPostFragment"
        android:name="com.example.instagram.fragments.SavedPostFragment"
        android:label="SavedPostFragment"
        tools:layout="@layout/fragment_saved_post">
        <action
            android:id="@+id/action_savedPostFragment_to_onePostFragment"
            app:destination="@id/onePostFragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right" />
    </fragment>
    <fragment
        android:id="@+id/editProfileFragment"
        android:name="com.example.instagram.fragments.EditProfileFragment"
        android:label="EditProfileFragment"
        tools:layout="@layout/fragment_edit_profile">
        <action
            android:id="@+id/action_editProfileFragment_to_profileFragment"
            app:destination="@id/profileFragment"
            app:enterAnim="@anim/enter_from_left"
            app:exitAnim="@anim/exit_to_right"
            app:popEnterAnim="@anim/enter_from_right"
            app:popExitAnim="@anim/exit_to_left"
            app:popUpTo="@id/profileFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/listFollowFragment"
        android:name="com.example.instagram.fragments.ListFollowFragment"
        android:label="fragment_list_follow"
        tools:layout="@layout/fragment_list_follow">
        <argument
            android:name="type"
            app:argType="string" />
        <argument
            android:name="profileId"
            app:argType="long" />
        <action
            android:id="@+id/action_listFollowFragment_to_profileFragment"
            app:destination="@id/profileFragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right"
            app:popUpTo="@id/profileFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/profilePictureFragment"
        android:name="com.example.instagram.fragments.ProfilePictureFragment"
        android:label="fragment_profile_picture"
        tools:layout="@layout/fragment_profile_picture" />
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.example.instagram.fragments.SettingsFragment"
        android:label="SettingsFragment"
        tools:layout="@layout/fragment_settings" />
    <fragment
        android:id="@+id/captionTagFragment"
        android:name="com.example.instagram.fragments.CaptionTagFragment"
        android:label="fragment_caption_tag"
        tools:layout="@layout/fragment_caption_tag" >
        <action
            android:id="@+id/action_captionTagFragment_to_tagFragment"
            app:destination="@id/tagFragment" />
        <action
            android:id="@+id/action_captionTagFragment_to_uploadDialog"
            app:destination="@id/uploadDialog" />
    </fragment>
    <fragment
        android:id="@+id/tagFragment"
        android:name="com.example.instagram.fragments.TagFragment"
        android:label="TagFragment"
        tools:layout="@layout/fragment_tag"

        />
    <dialog
        android:id="@+id/uploadDialog"
        android:name="com.example.instagram.dialog.UploadDialog"
        android:label="UploadDialog" />
</navigation>